    <link rel="stylesheet" href="{{ asset('front/css/nice-select.css') }}">
    <link rel="stylesheet" href="{{ asset('front/css/jquery.animatedheadline.css') }}">
    <link rel="stylesheet" href="{{ asset('front/css/main.css') }}">

    <link rel="shortcut icon" href="{{ asset('front/images/favicon.png') }} type="image/x-icon">
    {% block stylesheets %}{% endblock %}
    <title>Boleto  - Online Ticket Booking Website HTML Template</title>


</head>

<body>
<!-- ==========Preloader========== -->
<div class="preloader">
    <div class="preloader-inner">
        <div class="preloader-icon">
            <span></span>
            <span></span>
        </div>
    </div>
</div>
<!-- ==========Preloader========== -->
<!-- ==========Overlay========== -->
<div class="overlay"></div>
<a href="#0" class="scrollToTop">
    <i class="fas fa-angle-up"></i>
</a>
<!-- ==========Overlay========== -->

<!-- ==========Header-Section========== -->
<header class="header-section">
    <div class="container">
        <div class="header-wrapper">
            <div class="logo">
                <a href="#">
                    <img src="{{ asset('front/images/logo/logo.png') }}" alt="logo">
                </a>
            </div>
            <ul class="menu">
                <li>
                    <a href="#0" class="active">{% trans %}Home {% endtrans %}</a>
                    <ul class="submenu">
                        <li>
                            <a href="#0" class="active">{% trans %}Home One {% endtrans %}</a>
                        </li>
                        <li>
                            <a href="index-2.html">{% trans %}Home Two{% endtrans %}</a>
                        </li>
                    </ul>
                </li>
               {% block categorie %}
               {% endblock %}
                <li>
                    <a href="#0">{% trans %}events{% endtrans %}</a>
                    <ul class="submenu">
                        <li>
                            <a href="events.html">Events</a>
                        </li>
                        <li>
                            <a href="event-details.html">{% trans %}Event Details{% endtrans %}</a>
                        </li>
                        <li>
                            <a href="event-speaker.html">{% trans %}Event Speaker{% endtrans %}</a>
                        </li>
                        <li>
                            <a href="event-ticket.html">Event Ticket</a>
                        </li>
                        <li>
                            <a href="event-checkout.html">Event Checkout</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="#0">{% trans %}sports{% endtrans %}</a>
                    <ul class="submenu">
                        <li>
                            <a href="sports.html">Sports</a>
                        </li>
                        <li>
                            <a href="sport-details.html">Sport Details</a>
                        </li>
                        <li>
                            <a href="sports-ticket.html">Sport Ticket</a>
                        </li>
                        <li>
                            <a href="sports-checkout.html">Sport Checkout</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="#0">pages</a>
                    <ul class="submenu">
                        <li>
                            <a href="about.html">{% trans %}About Us{% endtrans %}</a>
                        </li>
                        <li>
                            <a href="apps-download.html">Apps Download</a>
                        </li>
                        <li>
                            <a href="sign-in.html">Sign In</a>
                        </li>
                        <li>
                            <a href="sign-up.html">Sign Up</a>
                        </li>
                        <li>
                            <a href="404.html">404</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="#0">blog</a>
                    <ul class="submenu">
                        <li>
                            <a href="blog.html">Blog</a>
                        </li>
                        <li>
                            <a href="blog-details.html">Blog Single</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="contact.html">contact</a>
                </li>
                <li class="header-button pr-0">
                    <a href="sign-up.html">join us</a>
                </li>
            </ul>
            <ul class="nav navbar-nav ml-auto">
                {% for locale in locales %}
                    {% if locale != app.request.locale %}
                        <li><a class="nav-link" href="{{ path('change_locale', {'locale': locale}) }}"><img style="height: 30px; width: 30px" src="{{ asset('front/images/'~locale~'.png') }}" alt="{{ locale }}">
                            </a></li>
                    {% endif %}
                {% endfor %}
            </ul>
            <div class="header-bar d-lg-none">
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
</header>
<!-- ==========Header-Section========== -->

<!-- ==========Banner-Section========== -->
<section class="banner-section">
    <div class="banner-bg bg_img bg-fixed" data-background="./assets/images/banner/banner01.jpg"></div>
    <div class="container">
        <div class="banner-content">
            <h1 class="title  cd-headline clip"><span class="d-block">book your</span> tickets for
                <span class="color-theme cd-words-wrapper p-0 m-0">
                        <b class="is-visible">Movie</b>
                        <b>Event</b>
                        <b>Sport</b>
                    </span>
            </h1>
            <p>Safe, secure, reliable ticketing.Your ticket to live entertainment!</p>
        </div>
    </div>
</section>
<!-- ==========Banner-Section========== -->
{% block body %}{% endblock %}

